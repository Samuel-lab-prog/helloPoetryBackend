			},
			"serviceFunc": {
				"name": "getUserPrivateInfo",
				"parameters": ["userId: number", "requesterId: number"],
				"returnTypes": ["FullUser[]", "null"]
			},
			"http": {
				"method": "GET",
				"path": "/:id/private-info",
				"query": ["includeSensitiveData: boolean"],
				"params": ["id: int"],
				"responsesCodes": ["200", "404", "403"],
				"needsAuth": true,
				"schemaName": "GetPrivateUserInfoSchema",
				"summary": "Retrieve private information of a user by ID",
