{
	"domain": "hey",
	"useCases": [
		{
			"name": "get-private-user-info",
			"type": "query",
			"dataModels": ["FullUser"],
			"errors": ["UserNotFoundError", "UnauthorizedError"],
			"repositoryMethods": [
				{
					"name": "selectFullUser",
					"parameters": ["userId: number"],
					"returnTypes": ["FullUser[]"]
				}
			],
			"useCaseFunc": {
				"name": "getPrivateUserInfo",
				"parameters": ["userId: number", "requesterId: number"],
