{
	"domain": "hey",
	"useCases": [
		{
			"name": "get-private-user-info",
			"type": "query",
			"dataModels": [
				{
					"name": "FullUser",
					"properties": {
						"id": "number",
						"username": "string",
						"email": "string",
						"fullName": "string",
						"address": "string",
						"phoneNumber": "string"
					}
				}
			],
			"errors": ["UserNotFoundError", "UnauthorizedError"],
			"repositoryMethods": [
				{
					"name": "selectFullUser",
					"parameters": [{ "name": "userId", "type": "number" }],
					"returnTypes": ["FullUser[]"]
				}
			],
			"useCaseFunc": {
				"name": "getPrivateUserInfo",
				"parameters": [{ "name": "userId", "type": "number" }],
				"returnTypes": ["FullUser[]", "null"]
			},
			"serviceFunc": {
				"name": "getPrivateUserInfo",
				"parameters": [
					{
						"name": "userId",
						"type": "number"
					}
				],
				"returnTypes": ["FullUser[]", "null"]
			},
			"http": {
				"method": "GET",
				"path": "/:id/private-info",
				"query": [{ "name": "includeSensitiveData", "type": "boolean" }],
				"params": [{ "name": "id", "type": "number" }],
				"responsesCodes": ["200", "404", "403"],
				"needsAuth": true,
				"schemaName": "GetPrivateUserInfoSchema",
				"summary": "Retrieve private information of a user by ID",
				"description": "This endpoint retrieves private information of a user specified by their ID. It requires authentication and may include sensitive data based on the query parameter.",
				"tags": ["Users", "Private Info"]
			}
		},
		{
			"name": "get-public-user-info",
			"type": "query",
			"dataModels": [
				{
					"name": "PartialUser",
					"properties": {
						"id": "number",
						"username": "string",
						"fullName": "string"
					}
				}
			],
			"errors": ["UserNotFoundError", "UnauthorizedError"],
			"repositoryMethods": [
				{
					"name": "selectPartialUser",
					"parameters": [{ "name": "userId", "type": "number" }],
					"returnTypes": ["PartialUser[]"]
				}
			],
			"useCaseFunc": {
				"name": "getPublicUserInfo",
				"parameters": [{ "name": "userId", "type": "number" }],
				"returnTypes": ["PartialUser[]", "null"]
			},
			"serviceFunc": {
				"name": "getPublicUserInfo",
				"parameters": [
					{
						"name": "userId",
						"type": "number"
					}
				],
				"returnTypes": ["PartialUser[]", "null"]
			},
			"http": {
				"method": "GET",
				"path": "/:id/public-info",
				"query": [{ "name": "includeSensitiveData", "type": "boolean" }],
				"params": [{ "name": "id", "type": "number" }],
				"responsesCodes": ["200", "404", "403"],
				"needsAuth": true,
				"schemaName": "GetPublicUserInfoSchema",
				"summary": "Retrieve public information of a user by ID",
				"description": "This endpoint retrieves public information of a user specified by their ID. It requires authentication and may include sensitive data based on the query parameter.",
				"tags": ["Users", "Public Info"]
			}
		}
	]
}
